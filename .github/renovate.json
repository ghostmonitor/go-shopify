{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":automergeDisabled",
    ":automergeMinor",
    "group:linters"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true
    }
  ],
  "hostRules": [
    {
      "matchHost": "https://github.com/ghostmonitor",
      "hostType": "go",
      "encrypted": {
        "token": ""
      }
    }
  ],
  "postUpdateOptions": [
    "gomodTidy"
  ]
}